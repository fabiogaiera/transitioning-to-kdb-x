#!/bin/bash

PORTS=(5010 5011 5012 5013 5014)

for PORT in "${PORTS[@]}"; do
  echo "Checking port $PORT..."
  PIDS=$(lsof -ti :$PORT)

  if [ -n "$PIDS" ]; then
    echo -e "Killing process(es) on port $PORT:"
    for pid in $PIDS; do
      echo "  $pid"
    done
    kill -9 $PIDS
  else
    echo "No process running on port $PORT"
  fi
done

echo "Done."
